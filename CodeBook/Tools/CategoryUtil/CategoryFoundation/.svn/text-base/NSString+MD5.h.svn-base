#import <Foundation/Foundation.h>

/**
 md5加密
 */
@interface NSString (MD5)

/**
 将字符串进行md5加密
 @return 返回加密后的字符串
 */
- (NSString *)md5sum;

/**
 将字符串进行md5加密
 @pamam inputString 输入的字符串
 @return 返回加密后的字符串
 */
+ (NSString *)md5:(NSString *)inputString;

/**
 将文件路径进行md5加密
 @pamam path 输入的文件路径
 @return 返回加密后的字符串
 */
+ (NSString *)fileMd5:(NSString *)path;

@end

